    "@babel/preset-typescript": "^7.1.0",
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-conventional": "^7.5.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.11",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme-to-json": "^3.3.5",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "^23.6.0",
    "mockdate": "^2.0.2",
    "prettier": "^1.16.3",
    "redux-mock-store": "^1.5.3",
    "semantic-release": "^15.13.3",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-processor-styled-components": "^1.5.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-microsoft-contrib": "^6.0.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.1",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@material-ui/core": "^3.9.1",
    "@material-ui/icons": "^3.0.2",
    "@types/dotenv": "^6.1.0",
    "@types/enzyme": "^3.1.16",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/luxon": "^1.10.2",
    "@types/react-inlinesvg": "^0.8.0",
    "@types/react-jss": "^8.6.1",
    "@types/react-redux": "^7.0.1",
    "@types/react-responsive": "^3.0.2",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-actions": "^2.3.1",
    "@types/redux-form": "^7.4.16",
    "@types/redux-promise": "^0.5.28",
    "@types/styled-components": "^4.1.6",
    "@types/webfontloader": "^1.6.29",
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-link": "^1.2.8",
    "apollo-link-context": "^1.0.14",
    "apollo-link-http": "^1.5.11",
    "connected-react-router": "^6.2.2",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "history": "^4.7.2",
    "jest-styled-components": "^6.3.1",
    "jss": "10.0.0-alpha.7",
    "luxon": "^1.10.0",
    "polished": "^2.3.3",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-apollo": "^2.4.1",
    "react-display-name": "^0.2.4",
    "react-dom": "^16.7.0",
    "react-ga": "^2.5.7",
    "react-hot-loader": "^4.6.5",
    "react-image-lightbox": "^5.1.0",
    "react-inlinesvg": "^0.8.4",
    "react-jss": "10.0.0-alpha.7",
    "react-markdown": "^4.0.6",
    "react-redux": "^6.0.0",
    "react-responsive": "^6.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-form": "^8.1.0",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.3",
    "uuid": "^3.3.2",
    "webfontloader": "^1.6.28"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testURL": "http://localhost",
    "coverageDirectory": "<rootDir>/reports/coverage",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.svg$": "<rootDir>/mocks/svgMock.js",
      "^Mocks/(.*)$": "<rootDir>/mocks/$1",
      "^appConfig$": "<rootDir>/config/app.js",
      "^Assets/(.*)$": "<rootDir>/assets/$1",
      "^Actions/(.*)$": "<rootDir>/src/actions/$1",
      "^Components/(.*)$": "<rootDir>/src/components/$1",
      "^Constants/(.*)$": "<rootDir>/src/constants/$1",
      "^Containers/(.*)$": "<rootDir>/src/containers/$1",
      "^Helpers/(.*)$": "<rootDir>/src/helpers/$1",
      "^Reducers/(.*)$": "<rootDir>/src/reducers/$1",
      "^Gql/(.*)$": "<rootDir>/src/gql/$1",
      "^ReduxLogger$": "redux-logger",
      "^ReduxDevtoolsExtension$": "redux-devtools-extension"
    },
    "setupFiles": [
      "<rootDir>/config/jest/setup.ts"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupFramework.ts",
    "testPathIgnorePatterns": [
      "<rootDir>/config/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "> 5%",
              "last 2 versions"
            ]
          }
        }
      ],
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "react-hot-loader/babel"
    ]
  },
  "release": {
    "branch": "staging",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      {
        "path": "@semantic-release/github",
        "githubUrl": "https://github.com/antokara/iloveyoutoomuch.fyi"
      }
    ],
    "prepare": [
      {
        "path": "@semantic-release/changelog",
        "changelogFile": "CHANGELOG.md"
      },
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): v${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      {
        "path": "@semantic-release/github",
        "githubUrl": "https://github.com/antokara/iloveyoutoomuch.fyi"
      }
    ],
    "npmPublish": false,
    "success": [],
    "fail": []
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recommended",
      "stylelint-config-styled-components"
    ]
  }
}
