{
  "name": "iloveyoutoomuch.fyi",
  "version": "1.0.0",
  "description": "Wedding Web Site",
  "main": "index.html",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack/development.js",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "build": "webpack --config ./config/webpack/production.js --env.NODE_ENV=production --env.production --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antokara/iloveyoutoomuch.fyi.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "react",
    "redux",
    "contentful",
    "PWA"
  ],
  "author": "Antonios Karagiannis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antokara/iloveyoutoomuch.fyi/issues"
  },
  "homepage": "https://github.com/antokara/iloveyoutoomuch.fyi#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/git": "^7.0.5",
    "@semantic-release/github": "^5.2.1",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^6.1.0",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.3.1",
    "react-hot-loader": "^4.3.12",
    "redux-mock-store": "^1.5.3",
    "semantic-release": "^15.12.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^3.6.0",
    "tslint-react-a11y": "^1.0.0",
    "typescript": "^3.1.6",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "tslint-blueprint": "^0.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testURL": "http://localhost",
    "coverageDirectory": "<rootDir>/reports/coverage",
    "moduleFileExtensions": [
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.svg$": "<rootDir>/mocks/svgMock.js",
      "^Mocks/(.*)$": "<rootDir>/mocks/$1",
      "^appConfig$": "<rootDir>/config/app.js",
      "^Actions/(.*)$": "<rootDir>/src/actions/$1",
      "^Components/(.*)$": "<rootDir>/src/components/$1",
      "^Constants/(.*)$": "<rootDir>/src/constants/$1",
      "^Containers/(.*)$": "<rootDir>/src/containers/$1",
      "^Helpers/(.*)$": "<rootDir>/src/helpers/$1",
      "^Reducers/(.*)$": "<rootDir>/src/reducers/$1",
      "^Assets/(.*)$": "<rootDir>/assets/$1",
      "^ReduxLogger$": "redux-logger",
      "^ReduxDevtoolsExtension$": "redux-devtools-extension"
    },
    "setupFiles": [
      "<rootDir>/config/setup.test.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupFramework.test.js",
    "testPathIgnorePatterns": [
      "<rootDir>/config/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "> 5%",
              "last 2 versions"
            ]
          }
        }
      ],
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "react-hot-loader/babel"
    ]
  },
  "release": {
    "branch": "staging",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      {
        "path": "@semantic-release/github",
        "githubUrl": "https://github.com/antokara/iloveyoutoomuch.fyi"
      }
    ],
    "prepare": [
      {
        "path": "@semantic-release/changelog",
        "changelogFile": "CHANGELOG.md"
      },
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): v${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      {
        "path": "@semantic-release/github",
        "githubUrl": "https://github.com/antokara/iloveyoutoomuch.fyi"
      }
    ],
    "npmPublish": false,
    "success": [],
    "fail": []
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
